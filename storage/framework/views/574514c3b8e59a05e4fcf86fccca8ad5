                    <a class="inactive-user-type" href="javascript:void(0);" id="activate_<?php echo e($roleClass); ?>seller">Seller</a>
                    <?php elseif( Session::get('last_login_role_id') == 2 ): ?>
                    <a class="inactive-user-type" href="javascript:void(0);" id="switchRole" data-role="1">Buyer</a>
                    <a href="javascript:void(0);" class="active-user-type" id="activate_<?php echo e($roleClass); ?>seller">Seller</a>
                    <?php endif; ?>
                 
                 <?php elseif( Auth::user()->lkp_role_id == SELLER ): ?>
                 	<?php if( Session::get('last_login_role_id') == 1 ): ?>
                    <a class="active-user-type" href="javascript:void(0);" id="activate_<?php echo e($roleClass); ?>buyer">Buyer</a>
                    <a class="inactive-user-type" href="javascript:void(0);" id="switchRole" data-role="2">Seller</a>
                    <?php elseif( Session::get('last_login_role_id') == 2 ): ?>
                    <a class="inactive-user-type" href="javascript:void(0);" id="activate_<?php echo e($roleClass); ?>buyer">Buyer</a>
                    <a href="javascript:void(0);" class="active-user-type" id="switchRole" data-role="2">Seller</a>
                    <?php endif; ?>
                 <?php endif; ?>
                
                    
                
                
                    
                    
               <?php endif; ?>  
                              
                    
					
					 

                </div>
            </div>
        </div>       
<?php endif; ?>
