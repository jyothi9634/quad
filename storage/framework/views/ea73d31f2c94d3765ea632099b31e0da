					$( "#amount" ).val(  ui.values[ 0 ] + "/-" + "     "+  ui.values[ 1 ] + "/-" );
					$( "#price_filter_pull_left" ).html(ui.values[ 0 ]);
					$( "#price_filter_pull_right" ).html(ui.values[ 1 ]);
				},
				change: function( event, ui ) {
					$(this).closest("form").submit();
				},
